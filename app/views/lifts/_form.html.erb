<div class="new_lift">
<%= form_for(@lift) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :departure %>
  <%= f.text_field :departure %>

  <%= f.label :destination %>
  <%= f.text_field :destination %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :open_seats %>
  <%= f.text_field :open_seats %>

  <%= f.label :departure_date %>
  <%= f.date_field :departure_date %>

  <%= f.submit %>
<% end %>
</div>
